   <dataset id="HF043">
      <alternateIdentifier>HF043</alternateIdentifier>
      <title>Carbon Biogeochemistry of Forested Headwater Streams at Harvard Forest 2006-2007</title>
      <creator>
         <individualName>
            <givenName>Peter</givenName>
            <surName>Raymond</surName>
         </individualName>
      </creator>
